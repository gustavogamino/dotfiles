# Personal Github Account
Host github.com
 Hostname ssh.github.com
 Port 443
 User git
 AddKeysToAgent yes
 IdentityFile ~/.ssh/personal-github-gustavogamino

# Accenture Bitbucket Account
Host accenture.com
 HostName altssh.innersource.accenture.com
 Port 443
 User git
 AddKeysToAgent yes
 IdentityFile ~/.ssh/accenture-bitbucket-gustavo.h.gamino
